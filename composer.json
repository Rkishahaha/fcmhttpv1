{
    "name": "appy/fcmhttpv1",
    "description": "FCM HTTP V1 packages",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Appy\\FcmHttpV1\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Appy Agency"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "google/cloud-binary-authorization": "^0.5.0",
        "google/apiclient": "^2.12.1"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=appyfcmhttpv1 --force"
        ]
    }
}